{
  "name": "ccboard",
  "displayName": "Cboard",
  "version": "0.1.0",
  "description": "AAC communication board with text-to-speech.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Unicef",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-browser": "^6.0.0",
