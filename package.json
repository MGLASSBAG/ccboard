    "electron-devtools-installer": "^3.2.0",
    "properties-parser": "0.3.1"
  },
  "devDependencies": {
    "cboard-speech-tts": "github:cboard-org/cboard-speech-tts",
    "com.lampa.startapp": "^6.1.6",
    "cordova-android": "^11.0.0",
    "cordova-clipboard": "^1.3.0",
    "cordova-electron": "^3.0.0",
    "cordova-ios": "^6.3.0",
    "cordova-plugin-android-permissions": "^1.1.3",
    "cordova-plugin-androidx": "^3.0.0",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-default-browser": "^1.0.0",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-file-opener2": "^2.2.1",
    "cordova-plugin-fullscreen": "^1.3.0",
    "cordova-plugin-google-analytics": "git+https://github.com/danwilson/google-analytics-plugin.git",
    "cordova-plugin-googleplus": "^8.5.2",
    "cordova-plugin-market": "git+https://github.com/xmartlabs/cordova-plugin-market.git",
    "cordova-plugin-iosrtc": "^8.0.3",
    "cordova-plugin-media": "^5.0.3",
    "cordova-plugin-media-capture": "^3.0.3",
    "cordova-plugin-network-information": "^3.0.0",
    "cordova-plugin-purchase": "git+https://github.com/j3k0/cordova-plugin-purchase.git#v13.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "electron-devtools-installer": "^3.1.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-android-permissions": {},
      "cordova-plugin-file": {},
      "cordova-plugin-file-opener2": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-google-analytics": {
        "GMS_VERSION": "11.0.1"
      },
      "cordova-plugin-statusbar": {},
      "cordova-plugin-fullscreen": {},
      "cordova-plugin-media": {
        "KEEP_AVAUDIOSESSION_ALWAYS_ACTIVE": "NO"
      },
      "cordova-plugin-media-capture": {},
      "git+https://github.com/cboard-org/cboard-speech-tts.git": {},
      "cordova-clipboard": {},
      "cordova-plugin-googleplus": {
        "PLAY_SERVICES_VERSION": "15.0.1",
        "REVERSED_CLIENT_ID": "${IOS_REVERSED_CLIENT_ID}",
        "WEB_APPLICATION_CLIENT_ID": "${IOS_WEB_APPLICATION_CLIENT_ID}"
      },
      "cordova-plugin-cboard-speech-tts": {},
      "cordova-plugin-market": {},
      "cordova-plugin-network-information": {},
      "com.lampa.startapp": {},
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-facebook-connect": {
        "APP_ID": "123456789",
        "APP_NAME": "myApplication",
        "FACEBOOK_URL_SCHEME_SUFFIX": " ",
        "OTHER_APP_SCHEMES": " ",
        "FACEBOOK_AUTO_LOG_APP_EVENTS": "true",
        "FACEBOOK_HYBRID_APP_EVENTS": "false",
        "FACEBOOK_ADVERTISER_ID_COLLECTION": "true",
        "FACEBOOK_ANDROID_SDK_VERSION": "11.3.0",
        "FACEBOOK_IOS_SDK_VERSION": "11.1.0",
        "FACEBOOK_BROWSER_SDK_VERSION": "v11.0"
      },
      "cordova-plugin-purchase": {},
      "cordova-plugin-iosrtc": {
        "MANUAL_INIT_AUDIO_DEVICE": "FALSE"
      },
      "cordova-plugin-default-browser": {}
    },
    "platforms": [
      "browser",
      "electron",
      "android",
      "ios"
    ]
  },
  "build": {
    "appx": {
      "identityName": "1817CboardAAC.AACCboard",
      "applicationId": "cboard",
      "publisher": "CN=4DBE72C9-499D-461E-AB05-588AF3AC6AFC",
      "publisherDisplayName": "Cboard AAC",
      "displayName": "AAC Cboard"
    }
  }
